{
  "maxerr":        30,
  "browserify":    true,
  "strict":        true,
  "latedef":       "nofunc",
  "module":        true,
  "node":          true,
  "noarg":         true,
  "unused":        "vars",
  "laxcomma":      true,
  "esnext":        true,
  "boss":          true,
  "indent":        2,
  "quotmark":      true,
